    <body class="login tooltips">
        <!--
        ===========================================================
        BEGIN PAGE
        ===========================================================
        -->
        <div class="login-header text-center">
            <img src="/template/assets/img/logo-login.png" class="logo" alt="Logo">
        </div>
        <div class="login-wrapper">
            <% if flash[:alert]%>
                <div class="alert alert-danger alert-bold-border fade in alert-dismissable">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <strong><%= flash[:alert] %></strong>
                </div>
            <% end %>
            
            <%= form_tag("/sessions",{method:"POST"}) do |f| %>
                <div class="form-group has-feedback lg left-feedback no-label">
                    <%= text_field :user, :username, class: "form-control no-border input-lg rounded", placeholder: "Enter username", autofocus: true %>
                    <span class="fa fa-user form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback lg left-feedback no-label">
                    <%= password_field :user, :password, class: "form-control no-border input-lg rounded", placeholder: "Enter password" %>
                    <span class="fa fa-unlock-alt form-control-feedback"></span>
                </div>
                <div class="form-group">
                    <%= submit_tag "LOGIN", class: "btn btn-warning btn-lg btn-perspective btn-block" %>
                </div>
            <% end %>
            
            <!--p class="text-center"><strong><a href="forgot-password.html">Forgot your password?</a></strong></p>
