<% if false %>
    <h1>Login Page</h1>

    <%= form_tag("/sessions",{method:"POST"}) do |f| %>
    
      <div class="field">
        Username<br>
        <%= text_field :user, :username %>
      </div>
      <div class="field">
        Password<br>
        <%= password_field :user, :password %>
      </div>
      <div class="actions">
        <%= submit_tag "Login" %>
      </div>
    <% end %>
<% end %>

