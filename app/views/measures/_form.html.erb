<%= input_select_required :measure, :user_id, User.all.collect {|p| [p.username, p.id]},{}, class: "form-control" %>
<%= input_select_required :measure, :patient_id, Patient.all.collect {|p| [p.name, p.id]},{}, class: "form-control" %>
<%= input_select_required :measure, :body_part_id, BodyPart.all.collect {|p| [p.name, p.id]},{}, class: "form-control" %>
<%= input_select_required :measure, :motion_id, Motion.all.collect {|p| [p.name, p.id]},{}, class: "form-control" %>
<%= input_text_field_required :measure, :measure_mode, class: "form-control" %>
<%= input_text_area :measure, :description, class: "form-control" %>
<%= input_datetime_select :measure, :measured_at, order: [:day, :month, :year], class: "form-control" %>